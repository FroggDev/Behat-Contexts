{
    "name": "froggdev/behat-installer",
    "type": "symfony-bundle",
    "description": "Easy Behat installation with new features and multiple reports options.",
    "keywords": ["symfony", "framework", "behat", "bdd", "tdd", "context", "frogg"],
    "license": "MIT",
    "authors": [
        {
            "name": "Marsiglietti Remy",
            "email": "admin@frogg.fr"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "ext-zip": "*",
        "Behat/Symfony2-extension": "dev-master",
        "behat/mink": "dev-master",
        "behat/mink-browserkit-driver": "dev-master",
        "behat/mink-extension": "dev-master",
        "behat/mink-goutte-driver": "dev-master",
        "behat/mink-selenium2-driver": "dev-master",
        "emuse/behat-html-formatter": "dev-master",
        "symfony/browser-kit": "*",
        "symfony/css-selector": "*",
        "symfony/debug-pack": "*",
        "symfony/dotenv": "*",
        "symfony/phpunit-bridge": "*",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "*",
        "froggdev/php-utils": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "froggdev\\BehatInstaller\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        }
    }
}
